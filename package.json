{
  "name": "TimeTable",
  "version": "1.6.3",
  "description": "Get your TimeTable in your menubar!",
  "license": "MIT",
  "author": "Willyb321",
  "repository": {
    "type": "git",
    "url": "https://github.com/willyb321/electron-menubar-timetable"
  },
  "bugs": {
    "url": "https://github.com/willyb321/electron-menubar-timetable/issues"
  },
  "main": "./main.js",
  "scripts": {
    "lint": "xo",
    "test": "npm run lint",
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build",
    "release": "build -m"
  },
  "devDependencies": {
    "electron": "^1.6.7",
    "xo": "^0.18.2",
    "electron-builder": "17.5.0"
  },
  "dependencies": {
    "electron-config": "^1.0.0",
    "electron-unhandled": "^1.0.0",
    "menubar": "^5.2.3",
    "moment": "^2.20.1",
    "tableify": "^1.1.0",
    "timetable.js": "git+https://github.com/Grible/timetable.js.git"
  },
  "xo": {
    "ignores": [
      "test.js"
    ]
  },
  "build": {
    "appId": "com.willyb321.electron-menubar-timetable"
  },
  "mac": {
    "target": [
      "dmg"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "deb"
      ]
    }
  },
  "category": "public.app-category.reference",
  "publish": [
    "github"
  ]
}
